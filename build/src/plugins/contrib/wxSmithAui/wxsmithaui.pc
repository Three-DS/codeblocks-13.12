prefix=/usr/local
exec_prefix=${prefix}
libdir=${exec_prefix}/lib/wxSmithAui
includedir=${prefix}/include/wxSmithAui
datarootdir = ${prefix}/share
sharedir=${datarootdir}

Name: wxSmithAui
Description: wxAUI support for wxSmith
Version: 13.12-9491
Libs: -L${libdir}
Cflags: -fPIC -I${includedir} \
			-I${includedir}/wxAuiManager \
			-I${includedir}/wxAuiNotebook \
			-I${includedir}/wxAuiToolBar
